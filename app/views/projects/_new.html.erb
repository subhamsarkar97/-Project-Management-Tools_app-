<%= render 'projects/navbar' %>
<section class="container">
    <%= render 'shared/error_messages' %>
    <p>Create New Project</p>
    <%= form_for ([@user, @user.projects.build]) do |project| %>
        <%= project.label "Project Title" %>
        <%= project.text_field :projectname, placeholder: "project title..." ,class: "form-control" %><br>
        <%= project.button :submit, :class => "btn btn-primary"%>
    <%end%>    
</section>